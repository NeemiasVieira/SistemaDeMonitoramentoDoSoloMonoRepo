{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "postbuild": "npx prisma generate"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/main.ts"
    }
  ],
  "includeFiles": ["node_modules/.prisma/client/**/*"]
}
